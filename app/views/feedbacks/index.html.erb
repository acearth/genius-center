<h1>Feedbacks</h1>

<table>
  <thead>
    <tr>
      <th>Email</th>
      <th>Info</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @feedbacks.each do |feedback| %>
      <tr>
        <td><%= feedback.email %></td>
        <td><%= feedback.info %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

