<p id="notice"><%= notice %></p>

<h1>Service Providers</h1>

<table>
  <thead>
    <tr>
      <th>App</th>
      <th>Auth level</th>
      <th>Credential</th>
      <th>Secret key</th>
      <th>User</th>
      <th>Description</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @service_providers.each do |service_provider| %>
      <tr>
        <td><%= service_provider.app_id %></td>
        <td><%= service_provider.auth_level %></td>
        <td><%= service_provider.credential %></td>
        <td><%= service_provider.secret_key %></td>
        <td><%= service_provider.user %></td>
        <td><%= service_provider.description %></td>
        <td><%= link_to 'Show', service_provider %></td>
        <td><%= link_to 'Edit', edit_service_provider_path(service_provider) %></td>
        <td><%= link_to 'Destroy', service_provider, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Service Provider', new_service_provider_path %>
